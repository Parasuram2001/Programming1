package replace;

public class vowels {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1="programming knowledge";
		String vowels=" ";
		for(int i=0;i<s1.length();i++)
		{
			char ch=s1.charAt(i);
			if(ch='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
			{
				vowels=vowels+ch;
			}
			else {
				System.out.println(ch);
			}
		}
		System.out.print(vowels+" ");

	}

}
