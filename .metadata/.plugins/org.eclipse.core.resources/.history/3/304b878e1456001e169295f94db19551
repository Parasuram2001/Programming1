package replace;

public class vowels {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1="programming knowledge";
		int vowelCount=0;
		for(int i=0;i<s1.length();i++)
		{
			char ch=s1.charAt(i);
			if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
			{
				vowelCount++;
			}
		}
		System.out.println(vowelCount);

	}

}
