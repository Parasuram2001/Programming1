package programming;

public class program {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String reverse=" ";
		String str="Pro gram ming";
		str=str.replaceAll(" ", "");
		for (int i=str.length()-1;i>=0;i--)
		{
			reverse=reverse+str.charAt(i);
		}
		System.out.println(reverse);

	}

}
